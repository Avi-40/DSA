// Function to reverse first k elements of a queue.
queue<int> modifyQueue(queue<int> q, int k) {
    // add code here.
    stack<int> s;
    for(int i=0;i<k;i++){
        int v=q.front();
        q.pop();
        s.push(v);
    }
    while(!s.empty()){
        int v=s.top();
        s.pop();
        q.push(v);
    }
    int i=q.size()-k;
    while(i--){
        int v=q.front();
        q.pop();
        q.push(v);
    }
    return q;
}
